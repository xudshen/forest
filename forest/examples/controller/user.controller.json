{
    "controllers": [
        {
            "id": "query_user_list",
            "parameters": [
                {
                    "name": "user_name",
                    "type": "string",
                    "required": false
                },
                {
                    "name": "first_name",
                    "type": "string",
                    "required": false
                },
                {
                    "name": "last_name",
                    "type": "string",
                    "required": false
                }
            ],
            "query": [
                {
                    "type": "sql",
                    "content": "select * from user where user_name = {user_name} and first_name = {first_name} and last_name = {last_name}"
                },
                {
                    "model": "user",
                    "filters": {
                        "op": "and",
                        "member": [
                            {
                                "op": "eq",
                                "field": "user_name",
                                "value": "{user_name}",
                                "value_not_blank": true
                            },
                            {
                                "field": "first_name",
                                "op": "eq",
                                "value": "{first_name}",
                                "value_not_blank": true
                            },
                            {
                                "field": "last_name",
                                "op": "eq",
                                "value": "{last_name}",
                                "value_not_blank": true
                            }
                        ]
                    }
                }
            ]
        }
    ]
}