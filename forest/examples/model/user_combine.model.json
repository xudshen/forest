    "src_type": "html",

    "models": [
        {
            "id": "user_combine",
            "meta": {
                "base_xpath": "/html/body/table[1]/",
                "fields": {
                    "first_name": {
                        "name": "thead/tr/th[2]/text()",
                        "filter": "string"
                    },
                    "last_name": {
