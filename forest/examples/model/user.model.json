{
    "models": [
        {
            "id": "user",
            "meta": {
                //like env setting
                //source:xxx, xxx is the data source
                "__base_xpath": "source:sample_user/html/body/table[1]/",
                "fields": {
                    "first_name": {
                        //{} means getting the value of this xpath
                        //[] is the filter of this xpath
                        "alias": "[string]{thead/tr/th[2]/text()}"
                    },
                    "last_name": {
                        //[string] can be ignored as the default filer
                        "alias": "{thead/tr/th[3]/text()}"
                    },
                    "user_name": {
                        "alias": "{thead/tr/th[4]/text()}",
                        "pk": true
                    }
                }
            },

            "databases": [
                {
                    "__base_xpath": "source:sample_user/html/body/table[1]/",
                    "first_name": "{tbody/tr/td[2]/text()}",
                    "last_name": "{tbody/tr/td[3]/text()}",
                    "user_name": "{tbody/tr/td[4]/text()}"
                }
            ],

            "preview:result": [
