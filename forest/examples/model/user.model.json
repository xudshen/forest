{
    "src": "https://raw.githubusercontent.com/xudshen/forest/master/forest/examples/sample-table.html",
    "src_type": "html",

    "models": [
        {
            "id": "user",
            "base_xpath": "/html/body/table/",
            "meta": {
                "fields": {
                    "first_name": "thead/tr/th[2]/text()",
                    "last_name": "thead/tr/th[3]/text()",
                    "user_name": "thead/tr/th[4]/text()"
                },
                "pk": "__user_name"
            },

            "__first_name": "(string)tbody/tr/td[2]/text()",
            "__last_name": "(string)tbody/tr/td[3]/text()",
            "__user_name": "(string)tbody/tr/td[4]/text()"
        }
    ]
}